# Frontend Production Environment Configuration
# Configure these variables for your production environment

# API Configuration
VITE_API_BASE_URL=https://api.ultrazend.com.br
VITE_WS_URL=wss://api.ultrazend.com.br

# Application Configuration
VITE_APP_NAME=UltraZend SMTP
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION=Professional SMTP Server & Email Management Platform

# Features Configuration
VITE_ENABLE_PWA=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# Analytics Configuration (optional)
# VITE_GOOGLE_ANALYTICS_ID=your-google-analytics-id
# VITE_SENTRY_DSN=your-sentry-dsn

# Security Configuration
VITE_ENABLE_CSP=true
VITE_SECURE_COOKIES=true